<h1>Listing wallets</h1>

<table>
  <tr>
    <th>User</th>
    <th>Deal</th>
    <th>Redempt</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @wallets.each do |wallet| %>
  <tr>
    <td><%= wallet.user_id %></td>
    <td><%= wallet.deal_id %></td>
    <td><%= wallet.redempt %></td>
    <td><%= link_to 'Show', api_v1_wallet_path(wallet) %></td>
    <td><%= link_to 'Edit', edit_api_v1_wallet_path(wallet) %></td>
    <td><%= link_to 'Destroy', api_v1_wallet_path(wallet), method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Wallet', new_api_v1_wallet_path %>
